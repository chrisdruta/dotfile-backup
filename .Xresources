Xft.dpi: 192
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

rofi.font: SourceCodePro 11

URxvt*cursorBlink:     true
URxvt*cursorUnderline: false
URxvt*font:            xft:Source Code Pro Medium:size=12
URxvt*boldFont:        xft:Source Code Pro Medium:size=12:bold
